class Animal {
    public void speak() {
        System.out.println("Generic animal sound");
    }
}

class Dog extends Animal {
    public void speak() {
        super.speak();  // Call parent class speak() method
        System.out.println("Woof!");
    }
}

public class Main {
    public static void main(String[] args) {
        Animal animal = new Animal();
        Dog dog = new Dog();

        animal.speak(); 
        dog.speak();     
                                 
    }
}
